<SequentialWorkflowActivity x:Class="WF3Workflows.Sequence2" x:Name="Sequence2" xmlns:ns0="clr-namespace:WF3Activities" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" xmlns="http://schemas.microsoft.com/winfx/2006/xaml/workflow">
	<ns0:Write x:Name="WriteBefore" Input="before" />
	<ns0:GetDuration x:Name="GetDuration1" />
	<DelayActivity x:Name="Delay1" TimeoutDuration="{ActivityBind GetDuration1,Path=Result}" />
	<ns0:Write x:Name="WriteAfter" Input="after" />
	<SequenceActivity x:Name="Seq1">
		<CodeActivity x:Name="Code1" ExecuteCode="MyCode" />
		<CancellationHandlerActivity x:Name="Cancellation1">
			<ns0:Write x:Name="Write1" Input="Cancelling!" />
		</CancellationHandlerActivity>
		<FaultHandlersActivity x:Name="FaultHandlersActivity1">
			<FaultHandlerActivity x:Name="FaultHandlerActivity1" FaultType="{x:Type p9:Exception}" xmlns:p9="clr-namespace:System;Assembly=mscorlib, Version=2.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089">
				<ns0:Write x:Name="Write2" Input="Exception!" />
				<ThrowActivity Fault="{ActivityBind FaultHandlerActivity1,Path=Fault}" x:Name="Throw1" />
			</FaultHandlerActivity>
		</FaultHandlersActivity>
	</SequenceActivity>
</SequentialWorkflowActivity>